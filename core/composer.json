{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "laravel/framework": "5.3.*",
        "barryvdh/laravel-ide-helper": "~2.0",
        "illuminate/html": "5.0.*@dev",
        "laracasts/flash": "~1.3",
        "cviebrock/image-validator": "2.0.*@alpha",
        "intervention/image": "dev-master",
        "barryvdh/laravel-dompdf": "^0.8.2",
        "webpatser/laravel-uuid": "1.5",
        "barryvdh/laravel-translation-manager": "0.2.9",
        "orangehill/iseed": "dev-master",
        "yajra/laravel-datatables-oracle": "6.29.0",
        "axdlee/laravel-config-writer": "^1.0",
        "guzzlehttp/guzzle": "^6.2",
        "torann/currency": "^1.0",
        "barryvdh/laravel-debugbar": "v2.4.0",
        "doctrine/dbal": "^2.8"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.1",
        "symfony/dom-crawler": "2.7.*",
        "symfony/css-selector": "2.7.*",
        "xethron/migrations-generator": "^2.0",
        "mercuryseries/flashy": "^1.0"
    },
    "repositories": {
        "packagist": { "url": "https://packagist.org", "type": "composer" }
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Inventory\\": "app/"
        },
        "files":[
            "app/Http/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php -r \"copy('.env.example', '.env');\"",
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
